<%- include('partials/navbar.ejs') %>

<div class="content-wrapper">
    <div class="container">

        <div class="col-md-12">
            <center><h4 class="header-line">Manage Pass</h4></center>
        </div>

        <div class="row">

            <div class="col-md-12">
                <div class="panel panel-success" style="border: 0;">
                     <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table" id="pasess">
                                <thead>
                                    <tr>
                                        <th>Category</th>
                                        <th>Registration</th>
                                        <th>Issue Date</th>
                                        <th>Name</th>
                                        <th>Address</th>
                                        <th>Phone</th>
                                        <th>Balance</th>
